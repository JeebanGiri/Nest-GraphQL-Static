{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "MY_API_KEY": "@my-api-key"
    },
    "redirects": [
        {
            "source": "/old-page",
            "destination": "/new-page",
            "statusCode": 301
        }
    ],
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=604800"
                }
            ]
        }
    ]
}